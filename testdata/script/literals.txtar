func shadowTest() {
	{
		var append, bool, string, fnc int
		_, _, _, _ = append, bool, string, fnc

		println("obfuscated with shadowed builtins (vars)")
	}
	{
		type append int
		type bool int
		type string int
		type fnc int

		println("obfuscated with shadowed builtins (types)")
	}
}
