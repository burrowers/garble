	tx := Transaction{
		Statement: Statement{
			Dest: dest,
		},
	}

	execute(tx)
}

func execute(db Transaction) {
	stmt := db.Statement

	v := reflect.TypeOf(stmt.Dest)

	fmt.Println(v)
